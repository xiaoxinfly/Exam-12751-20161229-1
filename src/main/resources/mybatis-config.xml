<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias alias="User" type="com.exam.pojo.User" />
		<typeAlias alias="UserInfo" type="com.exam.pojo.UserInfo" />
		<typeAlias alias="UserInfoHasUser" type="com.exam.pojo.UserInfoHasUser" />
		<typeAlias alias="UserList" type="com.exam.pojo.UserList" />
	</typeAliases>

	<plugins>
		<!-- 配置分页插件 -->
		<plugin interceptor="com.github.pagehelper.PageHelper">

			<!-- 设置为哪种数据库 Oracle\MySQL\MariaDB\SQLite\Hsqldb\PostgreSQL -->
			<property name="dialect" value="mysql" />
		</plugin>
	</plugins>
</configuration>